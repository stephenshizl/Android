+    private void doImportExport(AccountWithDataSet fromaccount, AccountWithDataSet toaccount1) {
+        final String importexportWhere = RawContacts.ACCOUNT_TYPE + "=?  AND " + RawContacts.ACCOUNT_NAME + "=?";
+        final String[] args = new String[]{fromaccount.type, fromaccount.name};
+        final AccountWithDataSet toaccount = toaccount1;
+        Log.i("ImportExport", "start import export thread");
+        new Thread(new Runnable() {
+            public void run() {
+                Cursor contactsCursor = null;
+                try {
+                    ContentResolver mContentResolver = mActivity.getContentResolver();
+                    contactsCursor = mContentResolver.query(Contacts.CONTENT_URI, new String[] {Contacts._ID}, importexportWhere, args, null);
+                    while(contactsCursor.moveToNext()) {
+                        importOneContact(mContentResolver, contactsCursor.getLong(0), toaccount);
+                    }
+                    Message msg = new Message();
+                    if(isImport) {
+                        msg.what = 1;
+                    } else {
+                        msg.what = 0;
+                    }
+                    msg.arg1 = 1;
+                    handler.sendMessage(msg);
+                } catch(Exception e){
+                    Message msg = new Message();
+                    if(isImport) {
+                        msg.what = 1;
+                    } else {
+                        msg.what = 0;
+                    }
+                    msg.arg1 = 0;
+                    handler.sendMessage(msg);
+                }finally {
+                    if (contactsCursor != null) {
+                        contactsCursor.close();
+                    }
+                    isProcess = false;
+                }
+            }
+        }).start();
+    }
+    private void importOneContact(ContentResolver cr, long contactid, AccountWithDataSet toaccount) {
+        ContentResolver mContentResolver = cr;
+        EntityIterator entityIterator = null;
+        Uri uri = RawContactsEntity.CONTENT_URI;
+        String selection = Data.CONTACT_ID + "=?";
+        long contactId = contactid;
+        String[] selectionArgs = new String[] {String.valueOf(contactId)};
+        final Map<String, List<ContentValues>> contentValuesListMap =
+                new HashMap<String, List<ContentValues>>();
+        Cursor dataResult;
+
+        //insert one list
+        final ArrayList<ContentProviderOperation> operationList =
+            new ArrayList<ContentProviderOperation>();
+        ContentProviderOperation.Builder builder =
+            ContentProviderOperation.newInsert(RawContacts.CONTENT_URI);
+        builder.withValue(RawContacts.ACCOUNT_NAME, toaccount.name);
+        builder.withValue(RawContacts.ACCOUNT_TYPE, toaccount.type);
+        operationList.add(builder.build());
+        try {
+            entityIterator = RawContacts.newEntityIterator(mContentResolver.query(
+                            uri, null, selection, selectionArgs, null));
+            Cursor data = mContentResolver.query(
+                            uri, null, selection, selectionArgs, null);
+            while (entityIterator.hasNext()) {
+                Entity entity = entityIterator.next();
+                for (NamedContentValues namedContentValues : entity.getSubValues()) {
+                    ContentValues contentValues = namedContentValues.values;
+                    String key = contentValues.getAsString(Data.MIMETYPE);
+                    if("vnd.android.cursor.item/group_membership".equals(key))
+                        continue;
+                    contentValues.remove(Data._ID);
+                    if (key != null) {
+                        List<ContentValues> contentValuesList =
+                                contentValuesListMap.get(key);
+                            builder = ContentProviderOperation.newInsert(Data.CONTENT_URI);
+                            builder.withValueBackReference(Data.RAW_CONTACT_ID, 0);
+                            builder.withValues(contentValues);
+                            operationList.add(builder.build());
+                    }
+                }
+            }
+            Log.i("ImportExport", "test contentValues operationList=" + operationList);
+            mContentResolver.applyBatch(ContactsContract.AUTHORITY, operationList);
+        } catch (RemoteException e) {
+            Log.e("ImportExport", String.format("%s: %s", e.toString(), e.getMessage()));
+        } catch (OperationApplicationException e) {
+            Log.e("ImportExport", String.format("%s: %s", e.toString(), e.getMessage()));
+        }finally {
+            if (entityIterator != null) {
+                entityIterator.close();
+            }
+        }
+        
+     }
+    //end
